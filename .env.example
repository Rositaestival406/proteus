# Proteus Environment Configuration Template
# Copy this file to .env and fill in your actual values
# IMPORTANT: .env file is gitignored and should NEVER be committed

# =============================================================================
# MalwareBazaar API Configuration
# =============================================================================
# Get your free API key from: https://bazaar.abuse.ch/api/
MALWAREBAZAAR_API_KEY=your_api_key_here

# =============================================================================
# Analysis Settings
# =============================================================================
THREAT_THRESHOLD=60.0
MAX_FILE_SIZE=104857600
MIN_STRING_LENGTH=4

# =============================================================================
# ML Model Paths
# =============================================================================
RF_MODEL_PATH=models/rf_model.pkl
ISO_MODEL_PATH=models/iso_model.pkl

# =============================================================================
# Dataset Paths
# =============================================================================
MALICIOUS_DATASET_DIR=dataset/malicious
CLEAN_DATASET_DIR=dataset/clean
TEST_DATASET_DIR=test_dataset

# =============================================================================
# Collection Settings
# =============================================================================
SAMPLES_PER_TAG=50
MAX_DOWNLOAD_RETRIES=2
DOWNLOAD_DELAY=2

# =============================================================================
# Logging
# =============================================================================
LOG_LEVEL=INFO
LOG_FILE=logs/proteus.log
VERBOSE=false

# =============================================================================
# Performance
# =============================================================================
PARALLEL_WORKERS=0
WORKER_MEMORY_LIMIT=512